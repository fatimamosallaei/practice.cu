    "def char_frequency(name):\n",
    "    dict = {}\n",
    "    for n in name:\n",
    "        keys = dict.keys()\n",
    "        if n in keys:\n",
    "            dict[n] += 1\n",
    "        else:\n",
    "            dict[n] = 1\n",
    "    return dict\n",
    "import pprint\n",
    "pprint.pprint(char_frequency('fatimamosallaeipooryazdi'))\n"
   ]
