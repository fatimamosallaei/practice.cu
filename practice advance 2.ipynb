    "def word_count(str):\n",
    "    counts= dict()\n",
    "    words=str.split()\n",
    "    for word in words:\n",
    "        if word in counts:\n",
    "            counts[word]+=1\n",
    "        else:\n",
    "            counts[word]=1\n",
    "    return counts\n",
    "print(word_count('slow progress is still progress '))\n",
    "        "
   ]
